<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.StackPane?>

<fx:root minHeight="40.0" minWidth="167.0" type="StackPane" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <HBox alignment="CENTER" style="">
         <children>
            <TextField fx:id="filePathTf" prefWidth="30.0" HBox.hgrow="ALWAYS">
               <HBox.margin>
                  <Insets right="1.0" />
               </HBox.margin>
            </TextField>
            <Button mnemonicParsing="false" onAction="#chooseFile" text="..." HBox.hgrow="ALWAYS" />
         </children>
      </HBox>
   </children>
   <padding>
      <Insets left="5.0" right="5.0" />
   </padding>
</fx:root>
